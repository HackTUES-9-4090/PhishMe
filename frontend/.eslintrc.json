{
	"extends": [
		"plugin:react/recommended",
		"eslint:recommended",
		"plugin:prettier/recommended"
	],
	"parserOptions": {
		"ecmaVersion": 2020,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"env": {
		"browser": true,
		"node": true,
		"es6": true
	},
	"plugins": ["react", "prettier"],
	"rules": {
		"prettier/prettier": "error",
		"react/prop-types": 0
	}
}
